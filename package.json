{
  "name": "eslint-plugin-sort",
  "description": "Auto-fixable sort rules for ESLint.",
  "version": "0.0.0-semantically-released",
  "author": "Mark Skelton",
  "repository": "github:mskelton/eslint-plugin-sort",
  "homepage": "https://github.com/mskelton/eslint-plugin-sort#readme",
  "bugs": {
    "url": "https://github.com/mskelton/eslint-plugin-sort/issues"
  },
  "license": "ISC",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sort",
    "fix"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules pnpm jest",
    "ts": "tsc"
  },
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./index.cjs"
  },
  "files": [
    "dist",
    "index.cjs"
  ],
  "peerDependencies": {
    "eslint": ">=6"
  },
  "dependencies": {
    "isomorphic-resolve": "^1.0.0",
    "@typescript-eslint/experimental-utils": "^5.49.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@mskelton/tsconfig": "^2.0.0",
    "@types/eslint": "^8.4.10",
    "@types/estree": "^1.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  }
}
